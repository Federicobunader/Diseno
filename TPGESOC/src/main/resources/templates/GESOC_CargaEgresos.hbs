<!-- partial:index.partial.html -->
<div class="form">
      
  <button type="submit" class="button button-back" onclick="window.location.href='/agregar_productos'">Volver atrás</button>

      <div class="tab-content">
        <div id="compra">
          <br><h3>Llene los datos correspondientes al egreso que desea cargar</h3><br>

          <form class="" action="/cargar_egreso" method="post" style="">

            <div class="field-wrap">
                <label>
                  Id Documento Comercial<span class="req">*</span>
                </label>
                  <input type="text"required autocomplete="off" id="CUIT" name="documentosComerciales" class="input-datos"/>
              </div>

              <h4>Fecha del egreso:</h4>
            <div class="field-wrap">
              <label>
                <span class="req"></span>
              </label>
                <input type="date"required autocomplete="off" id="CUIT" name="fechaDeOperacion" class="input-datos"/>
            </div>

          <div class="field-wrap">
            <div class="custom-select" style="width:200px;">
              <select name="medioDePago" id="tiposector" onChange=changetextbox()>
                <option value="0">Seleccione el medio de pago:</option>
                <option value="1">Tarjeta de crédito</option>
                <option value="2">Tarjeta de débito</option>
                <option value="3">Ticket</option>
                <option value="4">ATM</option>
                <option value="5">Efectivo</option>
              </select>

            </div>
          </div>

            <button type="submit" class="button button-back" onclick="window.location.href='/presupuestos'">Seguir con presupuestos</button>

          </form>


</div>

        <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script><script  src="/js/GESOC_CargarEgresos.js"></script>

